[
  {
    "key": "ctrl+shift+alt+d",
    "command": "workbench.action.duplicateWorkspaceInNewWindow"
  },
  { "key": "ctrl+shift+n", "command": "explorer.newFile", "when": "explorerViewletFocus" },
  { "key": "ctrl+shift+d", "command": "explorer.newFolder", "when": "explorerViewletFocus" },
  { "key": "ctrl+shift+g", "command": "git.checkout" },
  {
    "key": "ctrl+down",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+up",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+up",
    "command": "cursorMove",
    "args": {
      "to": "up",
      "by": "line",
      "value": 5
    },
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+down",
    "command": "cursorMove",
    "args": {
      "to": "down",
      "by": "line",
      "value": 5
    },
    "when": "editorTextFocus"
  }
]
